SetLogonBackground=0

; Computer - SHIDI_SERVER
[CLSID\{20D04FE0-3AEA-1069-A2D8-08002B30309D}\DefaultIcon]
DefaultValue=%SystemRoot%\Resources\Themes\everforest\Icons\pc.ico

; UsersFiles - SHIDI_USERFILES
[CLSID\{59031A47-3F72-44A7-89C5-5595FE6B30EE}\DefaultIcon]
DefaultValue==%SystemRoot%\Resources\Themes\everforest\Icons\user.ico

[Control Panel\Cursors]
AppStarting=%ResourceDir%\Themes\everforest\cursors\dark\busy.ani
Arrow=%ResourceDir%\Themes\everforest\cursors\dark\arrow.cur
Crosshair=%ResourceDir%\Themes\everforest\cursors\dark\cross.cur
Hand=%ResourceDir%\Themes\everforest\cursors\dark\link.cur
Help=%ResourceDir%\Themes\everforest\cursors\dark\helpsel.cur
IBeam=%ResourceDir%\Themes\everforest\cursors\dark\ibeam.cur
No=%ResourceDir%\Themes\everforest\cursors\dark\unavail.cur
NWPen=%ResourceDir%\Themes\everforest\cursors\dark\pen.cur
SizeAll=%ResourceDir%\Themes\everforest\cursors\dark\move.cur
SizeNESW=%ResourceDir%\Themes\everforest\cursors\dark\nesw.cur
SizeNS=%ResourceDir%\Themes\everforest\cursors\dark\ns.cur
SizeNWSE=%ResourceDir%\Themes\everforest\cursors\dark\nwse.cur
SizeWE=%ResourceDir%\Themes\everforest\cursors\dark\ew.cur
UpArrow=%ResourceDir%\Themes\everforest\cursors\dark\up.cur
Wait=%ResourceDir%\Themes\everforest\cursors\dark\wait.ani
